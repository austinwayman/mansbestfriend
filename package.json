{
  "name": "mansbestfriend",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "bcryptjs": "^2.4.3",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "jwt-simple": "^0.5.5",
    "mongoose": "^5.0.17",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "dotenv": "^6.2.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/austinwayman/mansbestfriend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/austinwayman/mansbestfriend/issues"
  },
  "homepage": "https://github.com/austinwayman/mansbestfriend#readme"
}
